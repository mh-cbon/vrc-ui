{
  "name": "@mh-cbon/vrc-ui",
  "version": "1.0.0",
  "description": "Vlc Remote Control UI",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "dev": "npm run backend & npm run frontend & wait",
    "frontend": "npm run bonjour-http & npm run watch & npm run http",
    "backend": "npm run vlc & npm run file-store",
    "bonjour-http": "bonjour-over-http -p 8090",
    "file-store": "npm run http-file-store & npm run http-file-store-bonjour",
    "http-file-store-bonjour": "bonjour-publish -H 127.0.0.1 -P 8091 -T http 'File-store HTTP server'",
    "http-file-store": "http-file-store",
    "vlc": "npm run vlc-http & npm run vlc-proxy & run npm vlc-bonjour",
    "vlc-bonjour": "bonjour-publish -H 127.0.0.1 -P 8080 -T http 'VLC HTTP server'",
    "vlc-http": "vlc -I http --http-password 123",
    "vlc-proxy": "node ../pico-proxy-OK/bin/cli.js --port 8083 --target http://127.0.0.1:8080/ --allow-origin http://127.0.0.1:8081 --allow-credentials",
    "vlc-proxy-ok": "pico-proxy --port 8083 --target http://127.0.0.1:8080/ --allow-origin http://127.0.0.1:8081 --allow-credentials",
    "watch": "webpack -w",
    "http": "http-server -o -p 8081"
  },
  "keywords": [
    "vlc"
  ],
  "author": "mh-cbon <mh-cbon@users.noreply.github.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "tag-loader": "^0.3.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "@mh-cbon/vlc-api": "github:mh-cbon/node-vlc-api",
    "async": "^1.5.2",
    "hyperquest": "^1.2.0",
    "jshashes": "^1.0.5",
    "numeral": "^1.5.3",
    "riot": "^2.3.15"
  }
}
